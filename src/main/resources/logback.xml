<configuration>
	<appender name="FILE" class="ch.qos.logback.core.FileAppender">
		<file>/tmp/logback-enabled.log</file>
		<encoder>
			<pattern>%d{HH:mm:ss.SSS} %-5level %logger{24} - %msg%n</pattern>
		</encoder>
	</appender>
	<root level="TRACE">
		<appender-ref ref="FILE"/>
	</root>
</configuration>
